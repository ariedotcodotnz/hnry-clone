{
  "name": "hnry-clone",
  "version": "1.0.0",
  "description": "HNRY Clone - React SPA deployed on Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist",
    "cf:dev": "wrangler pages dev dist --port 8788"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.5",
    "react-hook-form": "^7.66.0",
    "react-qr-code": "^2.0.12",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "classnames": "^2.3.2",
    "jquery": "^3.7.1",
    "@hotwired/stimulus": "^3.2.1",
    "@hotwired/turbo": "^7.3.0",
    "@rails/actioncable": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/lodash": "^4.14.195",
    "@types/jquery": "^3.5.16",
    "@types/date-fns": "^2.6.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.5.3",
    "vite": "^4.0.0",
    "wrangler": "^3.0.0"
  }
}
